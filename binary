#include<stdio.h>
int binary(int a[],int n,int target)
{
    int low=0,high=n-1;
    while(low<=high)
    {
        int mid=low + (high - low)/2;
        if(a[mid]==target)
        return mid;
        else if(a[mid]<target)
        low=mid+1;
        else
        high=mid-1;
    }
    return 0;
}

int main()
{
int n,target; 

printf("enter number of element:");
scanf("%d",&n);
int a[n];
printf("vale of element");
for(int i=0;i<n;i++)

    scanf("%d",&a[i]);
    printf("enter element to be searched");
    scanf("%d",&target);
    int result = binary(a,n,target);
    if(result== -1)
    printf("element not found");
    else 
    printf("element found%d",result);

    return 0;
}